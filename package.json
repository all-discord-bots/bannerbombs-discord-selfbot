    "country-data": "^0.0.31",
    "dateformat": "^3.0.3",
    "didyoumean2": "^3.0.0",
    "discord-emoji": "^1.1.1",
    "discord.js": "^11.3.2",
    "env-paths": "^2.0.0",
    "figlet": "^1.2.0",
    "fs-extra": "^7.0.1",
    "google-translate-api": "^2.3.0",
    "got": "^9.6.0",
    "inquirer": "^6.2.1",
    "math-expression-evaluator": "^1.2.16",
