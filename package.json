{
  "name": "sharpbot",
  "version": "0.0.9",
  "description": "A Discord.js selfbot written by Rayzr - 60 second installation!",
  "main": "src/bot.js",
  "scripts": {
    "start": "./bin/sharpbot",
    "background": "screen -dmS sharpbot ./bin/sharpbot",
